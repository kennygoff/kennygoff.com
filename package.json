{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "dev:netlify": "cross-env NODE_ENV=development node netlify/dev-server.js",
    "build": "react-router build && node netlify/prepare.js",
    "debug": "NODE_OPTIONS='--inspect-brk' react-router dev",
    "start": "netlify serve",
    "typecheck": "react-router typegen && ./patch-typegen.sh && tsc --build --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "clean": "rm -rf ./build"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mdx-js/rollup": "^3.1.1",
    "@netlify/functions": "^5.1.1",
    "@react-router/fs-routes": "^7.11.0",
    "@react-router/node": "^7.9.5",
    "cross-env": "^10.1.0",
    "fathom-client": "^3.7.2",
    "isbot": "^5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.9.5",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@react-router/dev": "^7.9.5",
    "@react-router/serve": "^7.9.5",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/eslint__js": "^9.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "happy-dom": "^20.0.11",
    "prettier": "3.7.4",
    "react-router-devtools": "^6.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-plugin-inspect": "^11.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.6"
  },
  "engines": {
    "node": ">=22"
  }
}
