{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "dev:netlify": "cross-env NODE_ENV=development node netlify/dev-server.js",
    "build": "react-router build && node netlify/prepare.js",
    "debug": "NODE_OPTIONS='--inspect-brk' react-router dev",
    "start": "netlify serve",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "clean": "rm -rf ./build"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mdx-js/rollup": "^3.1.0",
    "@netlify/functions": "^3.0.1",
    "@react-router/fs-routes": "^7.3.0",
    "@react-router/node": "^7.1.5",
    "cross-env": "^7.0.3",
    "fathom-client": "^3.7.2",
    "isbot": "^5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.0.0",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@react-router/dev": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.12",
    "@types/eslint__js": "^9.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "happy-dom": "^17.4.2",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^4.0.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.1",
    "vite-plugin-inspect": "^11.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=22"
  }
}
