{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "dev:netlify": "cross-env NODE_ENV=development node netlify/dev-server.js",
    "build": "react-router build && node netlify/prepare.js",
    "debug": "NODE_OPTIONS='--inspect-brk' react-router dev",
    "start": "netlify serve",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "clean": "rm -rf ./build"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mdx-js/rollup": "^3.1.1",
    "@netlify/functions": "^4.2.5",
    "@react-router/fs-routes": "^7.9.1",
    "@react-router/node": "^7.5.3",
    "cross-env": "^10.0.0",
    "fathom-client": "^3.7.2",
    "isbot": "^5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.5.3",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@react-router/dev": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@tailwindcss/typography": "^0.5.18",
    "@tailwindcss/vite": "^4.1.13",
    "@types/eslint__js": "^9.14.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "happy-dom": "^18.0.1",
    "prettier": "3.6.2",
    "react-router-devtools": "^5.1.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.0.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-plugin-inspect": "^11.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=22"
  }
